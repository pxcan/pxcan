{
  "name": "pxcan",
  "version": "0.0.3",
  "description": "ESM-ready pixel library",
  "keywords": [
    "pixel",
    "retro",
    "canvas",
    "sprite",
    "recolor",
    "palette"
  ],
  "homepage": "https://github.com/shamblesides/pxcan",
  "bugs": "https://github.com/shamblesides/pxcan/issues",
  "license": "MIT",
  "author": "Nigel Nelson",
  "files": [
    "src"
  ],
  "browser": "src/pxcan.js",
  "directories": {
    "lib": "src",
    "example": "example"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shamblesides/pxcan.git"
  },
  "scripts": {
    "lint": "eslint ."
  },
  "browserslist": [
    "last 2 versions and not dead and > 0.5% in US",
    "not ie 11",
    "ios >=9.3"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:compat/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true
    }
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-compat": "^3.1.1"
  }
}
