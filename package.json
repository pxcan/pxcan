{
  "name": "pxcan",
  "version": "0.0.13",
  "description": "ESM-ready pixel library",
  "keywords": [
    "pixel",
    "retro",
    "canvas",
    "sprite",
    "recolor",
    "palette"
  ],
  "homepage": "https://github.com/shamblesides/pxcan",
  "bugs": "https://github.com/shamblesides/pxcan/issues",
  "license": "MIT",
  "author": "Nigel Nelson",
  "files": [
    "*.js",
    "fonts",
    "util"
  ],
  "main": "index.js",
  "browser": "index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shamblesides/pxcan.git"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "ava"
  },
  "browserslist": [
    "last 2 versions and not dead and > 0.5%",
    "not operamini all",
    "not ie 11",
    "ios >=9.3"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:compat/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true
    }
  },
  "ava": {
    "require": [
      "esm",
      "./test/_setup-browser-env.js"
    ]
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "browser-env": "^3.2.6",
    "canvas": "^2.5.0",
    "eslint": "^4.19.1",
    "eslint-plugin-compat": "^3.1.1",
    "esm": "^3.2.25"
  }
}
